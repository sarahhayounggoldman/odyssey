    <div>
        <% if (posts.length > 0) { %>
            <% posts.forEach(post => { %>
                <%- include('post', { post: post, username: username }) %>
            <% }); %>
        <% } else { %>
            <p>No posts yet.</p>
        <% } %>
    </div>

